{
    "i18n-ally.localesPaths": "scripts/locales",
    "i18n-ally.sourceLanguage": "en",
    "i18n-ally.sortKeys": true,
    "github.copilot.enable": {
        "typescript": false
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "pnpm-lock.yaml": true,
        "**/dist": true
    },
    "i18n-ally.keystyle": "flat",
    "typescript.tsdk": "node_modules/typescript/lib",
    "prettier.semi": false,
    "typescript.format.semicolons": "remove",
    "javascript.format.semicolons": "remove",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#cfbee0",
        "activityBar.background": "#cfbee0",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#ad8052",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#cfbee0",
        "statusBar.background": "#b59ccf",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#9b7abe",
        "statusBarItem.remoteBackground": "#b59ccf",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#b59ccf",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#b59ccf99",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#b59ccf"
}
